{
  "metadata": [{
    "src": [{
      "files": "Saritasa.Tools.Common/**.cs",
      "exclude": ["**/obj/**", "**/bin/**"],
      "src": "../src"
    }],
    "dest": "api/common"
  },
  {
    "src": [{
      "files": "Saritasa.Tools.Domain/**.cs",
      "exclude": ["**/obj/**", "**/bin/**"],
      "src": "../src"
    }],
    "dest": "api/domain"
  },
  {
    "src": [{
      "files": "Saritasa.Tools.Messages.Abstractions/**.cs",
      "exclude": ["**/obj/**", "**/bin/**"],
      "src": "../src"
    }],
    "dest": "api/messages"
  },
  {
    "src": [{
      "files": "Saritasa.Tools.Emails/**.cs",
      "exclude": ["**/obj/**", "**/bin/**"],
      "src": "../src"
    }],
    "dest": "api/emails"
  }],
  "build": {
    "content": [{
        "files": [
          "api/**.yml"
        ]
      },
      {
        "files": [
          "/toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [{
        "files": [
          "images/**"
        ]
    }],
    "overwrite": [{
      "files": [
        "**.md"
      ],
      "exclude": [
        "obj/**",
        "_site/**"
      ]
    }],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
}
